    <class>com.club.BEANS.Funcionario</class>
    <class>com.club.BEANS.Marcas</class>
    <class>com.club.BEANS.Parametros</class>
    <class>com.club.BEANS.Campanasms</class>
    <class>com.club.BEANS.Sms</class>
    <class>com.club.BEANS.RespuestaSMS</class>
    <class>com.club.BEANS.CampEconomica</class>
