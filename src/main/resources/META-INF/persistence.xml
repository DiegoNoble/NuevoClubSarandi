    <provider>org.hibernate.ejb.HibernatePersistence</provider>
    <class>com.club.BEANS.Caja</class>
    <class>com.club.BEANS.Categoria</class>
    <class>com.club.BEANS.Cobrador</class>
    <class>com.club.BEANS.Dependiente</class>
    <class>com.club.BEANS.FichaMedica</class>
    <class>com.club.BEANS.Mensualidades</class>
    <class>com.club.BEANS.Rubro</class>
    <class>com.club.BEANS.Sectores</class>
    <class>com.club.BEANS.Socio</class>
    <class>com.club.BEANS.Usuario</class>
    <class>com.club.BEANS.CcCobrador</class>
