    <class>com.club.BEANS.Numeros</class>
    <class>com.club.BEANS.SectoresFuncionario</class>
    <class>com.club.BEANS.VentaCampEco</class>
    <properties>
      <property name="hibernate.cache.provider_class" value="org.hibernate.ejb.HibernatePersistence"/>
      <property name="hibernate.hbm2ddl.auto" value="update"/>
      <property name="hibernate.format_sql" value="true"/>
      <property name="hibernate.show_sql" value="true"/>
    </properties>
  </persistence-unit>
</persistence>
